{
  "functions": {
    "api/notion.js": {
      "maxDuration": 10
    },
    "api/contact.js": {
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/linkedin",
      "destination": "https://www.linkedin.com/in/harmanpunn/",
      "permanent": true
    },
    {
      "source": "/github",
      "destination": "https://github.com/harmanpunn",
      "permanent": true
    },
    {
      "source": "/",
      "destination": "https://www.linkedin.com/in/harmanpunn/",
      "has": [
        {
          "type": "host",
          "value": "linkedin.harmanpunn.me"
        }
      ],
      "permanent": true
    },
    {
      "source": "/",
      "destination": "https://github.com/harmanpunn",
      "has": [
        {
          "type": "host",
          "value": "github.harmanpunn.me"
        }
      ],
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/blog",
      "destination": "/index.html"
    },
    {
      "source": "/blog/:slug",
      "destination": "/index.html"
    }
  ]
} 